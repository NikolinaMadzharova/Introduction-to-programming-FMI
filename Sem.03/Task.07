#include <iostream>
using namespace std;

int main()
{
	int n;
	cin >> n;
    for (int i = n; i>1; i--) {
        //Check if i is prime
        bool isPrime = true;
        double sqrtOfN = sqrt(i);
        for(int k = 2; k <= sqrtOfN; k++) {
            if (i% k == 0) {
                isPrime = false;
                break;
            }
        }
        if (!isPrime) {
            continue; //don't need this number
        }
          //check how many times i devides n
        int count = 0;
        while (n % i == 0) {
            count++;
            n /= i;
        }
        if (count >= 1) {
            cout << i<<"^"<<count<<" ";
            
        }

    }
}
